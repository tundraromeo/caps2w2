{
  "name": "caps2e2/inventory-system",
  "description": "Inventory Management System with secure environment configuration",
  "type": "project",
  "require": {
    "php": ">=7.4",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "Api/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
